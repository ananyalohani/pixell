#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/mocha@7.2.0/node_modules/mocha/bin/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/mocha@7.2.0/node_modules/mocha/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/mocha@7.2.0/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/mocha@7.2.0/node_modules/mocha/bin/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/mocha@7.2.0/node_modules/mocha/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/mocha@7.2.0/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mocha@7.2.0/node_modules/mocha/bin/mocha" "$@"
else
  exec node  "$basedir/../../../../../mocha@7.2.0/node_modules/mocha/bin/mocha" "$@"
fi
