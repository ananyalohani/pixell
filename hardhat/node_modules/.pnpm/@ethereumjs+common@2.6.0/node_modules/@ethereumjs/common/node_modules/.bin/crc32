#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/crc-32@1.2.0/node_modules/crc-32/bin/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/crc-32@1.2.0/node_modules/crc-32/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/crc-32@1.2.0/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/@ethereumjs+common@2.6.0/node_modules/@ethereumjs/common/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/@ethereumjs+common@2.6.0/node_modules/@ethereumjs/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/@ethereumjs+common@2.6.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/crc-32@1.2.0/node_modules/crc-32/bin/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/crc-32@1.2.0/node_modules/crc-32/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/crc-32@1.2.0/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/@ethereumjs+common@2.6.0/node_modules/@ethereumjs/common/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/@ethereumjs+common@2.6.0/node_modules/@ethereumjs/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/@ethereumjs+common@2.6.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../crc-32@1.2.0/node_modules/crc-32/bin/crc32.njs" "$@"
else
  exec node  "$basedir/../../../../../../crc-32@1.2.0/node_modules/crc-32/bin/crc32.njs" "$@"
fi
