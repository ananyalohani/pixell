#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules/node-gyp-build/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/secp256k1@4.0.2/node_modules/secp256k1/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/secp256k1@4.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules/node-gyp-build/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node-gyp-build@4.3.0/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/secp256k1@4.0.2/node_modules/secp256k1/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/secp256k1@4.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-gyp-build@4.3.0/node_modules/node-gyp-build/bin.js" "$@"
else
  exec node  "$basedir/../../../../../node-gyp-build@4.3.0/node_modules/node-gyp-build/bin.js" "$@"
fi
