#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/solc@0.7.3_debug@4.3.3/node_modules/solc/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/solc@0.7.3_debug@4.3.3/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules/solc/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/solc@0.7.3_debug@4.3.3/node_modules/solc/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/solc@0.7.3_debug@4.3.3/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules/solc/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solc/solcjs" "$@"
else
  exec node  "$basedir/../solc/solcjs" "$@"
fi
