#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/flat@4.1.1/node_modules/flat/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/flat@4.1.1/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/yargs-unparser@1.6.0/node_modules/yargs-unparser/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/yargs-unparser@1.6.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/flat@4.1.1/node_modules/flat/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/flat@4.1.1/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/yargs-unparser@1.6.0/node_modules/yargs-unparser/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/yargs-unparser@1.6.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../flat@4.1.1/node_modules/flat/cli.js" "$@"
else
  exec node  "$basedir/../../../../../flat@4.1.1/node_modules/flat/cli.js" "$@"
fi
