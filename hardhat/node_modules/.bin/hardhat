#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/internal/cli/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/internal/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/hardhat/internal/cli/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/hardhat/internal/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/hardhat/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/internal/cli/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/internal/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/hardhat@2.7.1/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/.pnpm/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules:/Users/ananyalohani/Desktop/pixell/node_modules:/Users/ananyalohani/Desktop/node_modules:/Users/ananyalohani/node_modules:/Users/node_modules:/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/hardhat/internal/cli/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/hardhat/internal/node_modules:/Users/ananyalohani/Desktop/pixell/hardhat/node_modules/hardhat/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/internal/cli/cli.js" "$@"
else
  exec node  "$basedir/../hardhat/internal/cli/cli.js" "$@"
fi
